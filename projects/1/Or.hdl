/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=i);
    Not(in=b, out=j);
    Nand(a=i, b=j, out=out);    
}
